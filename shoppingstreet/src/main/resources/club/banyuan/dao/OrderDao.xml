<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper     PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="club.banyuan.dao.OrderDao">
    <select id="addOrder" parameterType="club.banyuan.entity.Order" resultType="club.banyuan.entity.Order">
        insert into shoppingStreet.order(userId,loginName,userAddress,createTime,cost,serialNumber )
        values(#{userId},#{loginName},#{userAddress},#{createTime},#{cost},#{serialNumber})
    </select>
</mapper>